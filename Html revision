   <p><img src="image.jpg" usemap="#mapname">
   <map name="mapname">
       <area shape="rect" coords="34,44,270,350" href="link.html">
   </map></p>
   
   <!-- Object -->
   <p><object data="file.swf" type="application/x-shockwave-flash"></object></p>
   
   <!-- Output -->
   <p>The result is: <output name="result">42</output></p>
   
   <!-- Quote -->
   <p><q>Inline quote</q></p>
   
   <!-- Sample Output -->
   <p><samp>Error: File not found</samp></p>
   
   <!-- Script -->
   <script>
       console.log('This is a script');
   </script>
   
